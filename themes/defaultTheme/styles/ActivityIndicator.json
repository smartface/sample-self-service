{
  ".activityIndicator": {
    "touchEnabled": true,
    "width": 42,
    "height": 42,
    "color": "#ADADAD",
    "visible": true,
    "backgroundColor": "#FFFFFF",
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "font": {},
    "flexProps": {
      "positionType": "RELATIVE",
      "alignSelf": "AUTO"
    },
    "minHeight": 21,
    "minWidth": 21,
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "backgroundColor": "#9F6000"
    },
    "&-error": {
      "backgroundColor": "#D8000C"
    },
    "&-success": {
      "backgroundColor": "#4F8A10"
    }
  }
}

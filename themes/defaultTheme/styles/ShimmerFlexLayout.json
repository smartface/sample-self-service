{
	".shimmerFlexLayout": {
		"height": 200,
		"width": 200,
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "#FFFFFF",
		"alpha": 1,
		"baseAlpha": 1,
		"pauseDuration": 400,
		"shimmeringDirection": "RIGHT",
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"font": {},
		"flexProps": {
			"alignSelf": "AUTO",
			"positionType": "RELATIVE"
		},
		"layout": {
			"flexProps": {
				"direction": "INHERIT",
				"alignContent": "STRETCH",
				"alignItems": "STRETCH",
				"justifyContent": "FLEX_START",
				"flexWrap": "NOWRAP",
				"flexDirection": "COLUMN"
			}
		},
		"ios": {
			"animationAlpha": 0.5,
			"beginFadeDuration": 100,
			"endFadeDuration": 300,
			"speed": 230

		},
		"android": {
			"highlightAlpha": 0.5,
			"highlightLength": 1
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	}
}
{
  ".videoView": {
    "height": 200,
    "width": 200,
    "visible": true,
    "backgroundColor": "#FFFFFF",
    "alpha": 1,
    "touchEnabled": true,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "font": {},
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "backgroundColor": "#9F6000"
    },
    "&-error": {
      "backgroundColor": "#D8000C"
    },
    "&-success": {
      "backgroundColor": "#4F8A10"
    }
  }
}
